{
	"name": "python-repl-paste-fixer",
	"displayName": "Python Repl Paste Fixer",
	"description": "Remove prefixes from code copied from Python REPL.",
	"version": "1.0.2",
	"publisher": "travie",
	"author": {
		"name": "Travis Vanderpool",
		"email": "travis.vanderpool@gmail.com"
	},
	"license": "AGPLv3",
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Formatters"
	],
	"main": "./out/extension.js",
	"activationEvents": [
		"onLanguage:python"
	],
	"contributes": {
		"commands": [
			{
				"command": "pyReplPasteFixer.pasteFixed",
				"title": "Paste with python repl prefixes removed"
			}
		],
		"keybindings": [
			{
				"win": "ctrl+shift+v",
				"linux": "ctrl+shift+v",
				"mac": "cmd+shift+v",
				"key": "ctrl+shift+v",
				"command": "pyReplPasteFixer.pasteFixed",
				"when": "editorTextFocus && editorLangId == 'python'"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"package": "vsce package"
	},
	"devDependencies": {
		"@types/node": "^16.18.34",
		"@types/vscode": "^1.74.0",
		"@typescript-eslint/eslint-plugin": "^6.7.0",
		"@typescript-eslint/parser": "^6.7.0",
		"@vscode/vsce": "^2.22.0",
		"eslint": "^8.50.0",
		"typescript": "^5.3.2"
	},
	"__metadata": {
		"installedTimestamp": 1703359190989
	}
}
